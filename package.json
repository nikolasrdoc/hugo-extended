{
  "name": "hugo-extended",
  "version": "0.67.0",
  "hugoVersion": "0.67.0",
  "description": "Plug-and-play binary wrapper for Hugo Extended, the awesomest static-site generator.",
  "homepage": "https://github.com/jakejarvis/hugo-extended#readme",
  "license": "MIT",
  "author": {
    "name": "Jake Jarvis",
    "email": "jake@jarv.is",
    "url": "https://github.com/jakejarvis"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jakejarvis/hugo-extended.git"
  },
  "keywords": [
    "hugo",
    "hugo extended",
    "gohugoio",
    "cli",
    "bin",
    "binary",
    "wrapper",
    "static site generator"
  ],
  "main": "index.js",
  "dependencies": {
    "bin-wrapper": "^4.1.0"
  },
  "devDependencies": {
    "bin-check": "^4.1.0",
    "mocha": "*",
    "xo": "*"
  },
  "scripts": {
    "postinstall": "node lib/install.js",
    "test": "xo && mocha"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "lib/*.js",
    "cli.js",
    "index.js"
  ],
  "bin": {
    "hugo": "cli.js"
  },
  "xo": {
    "envs": [
      "es6",
      "node"
    ],
    "space": true,
    "rules": {
      "object-curly-spacing": "off",
      "promise/prefer-await-to-then": "warn"
    }
  }
}
