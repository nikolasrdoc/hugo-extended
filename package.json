{
  "name": "hugo-extended",
  "version": "0.63.2",
  "hugoVersion": "0.63.2",
  "description": "Plug-and-play binary wrapper for Hugo Extended, the awesomest static-site generator.",
  "homepage": "https://github.com/jakejarvis/hugo-node#readme",
  "license": "MIT",
  "author": {
    "name": "Jake Jarvis",
    "email": "jake@jarv.is",
    "url": "https://github.com/jakejarvis"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jakejarvis/hugo-node.git"
  },
  "keywords": [
    "hugo",
    "hugo extended",
    "gohugoio",
    "cli",
    "bin",
    "binary",
    "wrapper",
    "static site generator"
  ],
  "main": "index.js",
  "dependencies": {
    "bin-wrapper": "^4.1.0",
    "rimraf": "^3.0.1",
    "signale": "^1.4.0"
  },
  "devDependencies": {
    "bin-check": "^4.1.0",
    "eslint": "^6.8.0",
    "mocha": "^7.0.1"
  },
  "scripts": {
    "eslint": "eslint .",
    "mocha": "mocha",
    "test": "npm run eslint && npm run mocha",
    "postinstall": "rimraf vendor && node lib/install"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "lib/*.js",
    "cli.js",
    "index.js"
  ],
  "bin": {
    "hugo": "cli.js"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "prefer-destructuring": [
        "error",
        {
          "object": true,
          "array": false
        }
      ],
      "strict": "error"
    }
  }
}
